[core]
	editor = vim
	preloadindex = true
	fscache = true
	autocrlf = windows:true/linux:input
	quotepath = false
[user]
	name = csucjh
	email = csucjh@163.com
[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	co = checkout
	br = branch
	st = status
	fp = format-patch
	rmt = remote
	cl = clone
	ci = commit
[gui]
	encoding = utf-8
[i18n]
	commitencoding = utf-8
	logoutputencoding = utf-8
[pull]
	rebase = true
[gc]
	auto = 256
[bash]
	showUntrackedFiles = true
	hideIfPwdIgnored = true
	showDirtyState = true
[http]
	sslVerify = false
